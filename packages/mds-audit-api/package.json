{
  "name": "@mds-core/mds-audit-api",
  "version": "0.3.24",
  "description": "Endpoint for auditing compliance with MDS",
  "keywords": [
    "mds",
    "audit"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/audit pnpm watch server",
    "test": "PATH_PREFIX=/audit DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.4.19",
    "@mds-core/mds-api-helpers": "workspace:0.2.15",
    "@mds-core/mds-api-server": "workspace:0.4.7",
    "@mds-core/mds-attachment-service": "workspace:0.5.16",
    "@mds-core/mds-db": "workspace:0.6.7",
    "@mds-core/mds-ingest-service": "0.10.0",
    "@mds-core/mds-logger": "workspace:0.5.6",
    "@mds-core/mds-providers": "workspace:0.2.8",
    "@mds-core/mds-schema-validators": "workspace:0.5.6",
    "@mds-core/mds-service-helpers": "workspace:0.6.5",
    "@mds-core/mds-types": "workspace:0.9.1",
    "@mds-core/mds-utils": "workspace:0.4.1",
    "@types/express": "4.17.13",
    "@types/multer": "1.4.7",
    "@types/sharp": "0.29.2",
    "aws-sdk": "2.999.0",
    "express": "4.17.1",
    "multer": "1.4.3",
    "sharp": "0.29.3"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.18"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
