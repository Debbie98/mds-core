{
  "name": "@mds-core/mds-policy-service",
  "version": "0.12.0",
  "description": "Mobility Data Specification Policy Service",
  "keywords": [
    "mds",
    "policy"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "jest --runInBand",
    "build": "pnpm _build",
    "start": "pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "node --experimental-repl-await -r ts-node/register -r ../../dotenv-flow.js",
    "typeorm": "typeorm-ts-node-commonjs -r ../../dotenv-flow.js -d ormconfig",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-geography-service": "workspace:0.10.23",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-repository": "workspace:0.2.1",
    "@mds-core/mds-rpc-common": "workspace:0.3.20",
    "@mds-core/mds-schema-validators": "workspace:0.5.11",
    "@mds-core/mds-service-helpers": "workspace:0.6.9",
    "@mds-core/mds-stream": "workspace:0.3.2",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.1",
    "pg": "8.7.3",
    "typeorm": "0.3.4",
    "typeorm-cursor-pagination": "0.8.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.29"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
