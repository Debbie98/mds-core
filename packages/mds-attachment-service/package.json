{
  "name": "@mds-core/mds-attachment-service",
  "version": "0.3.6",
  "description": "Mobility Data Specification Attachment Service",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-logger": "workspace:0.5.0",
    "@mds-core/mds-repository": "workspace:0.1.23",
    "@mds-core/mds-rpc-common": "workspace:0.2.5",
    "@mds-core/mds-schema-validators": "workspace:0.3.13",
    "@mds-core/mds-service-helpers": "workspace:0.4.4",
    "@mds-core/mds-types": "workspace:0.7.1",
    "@mds-core/mds-utils": "workspace:0.2.11",
    "@types/sharp": "0.29.2",
    "aws-sdk": "2.999.0",
    "express": "4.17.1",
    "joi": "17.4.0",
    "multer": "1.4.3",
    "pg": "8.7.1",
    "sharp": "0.29.1",
    "typeorm": "0.2.40"
  }
}
