{
  "name": "@mds-core/mds-collector-backend",
  "version": "0.1.8",
  "description": "Mobility Data Specification Collector Backend Service",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-logger": "0.2.3",
    "@mds-core/mds-repository": "0.1.8",
    "@mds-core/mds-rpc-common": "0.1.8",
    "@mds-core/mds-schema-validators": "0.2.3",
    "@mds-core/mds-service-helpers": "0.3.1",
    "@mds-core/mds-stream": "0.1.34",
    "@mds-core/mds-types": "0.3.2",
    "@mds-core/mds-utils": "0.1.34",
    "ajv": "8.6.1",
    "typeorm": "0.2.34"
  }
}
