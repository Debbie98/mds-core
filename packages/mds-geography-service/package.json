{
  "name": "@mds-core/mds-geography-service",
  "version": "0.7.1",
  "description": "Mobility Data Specification Geography Service",
  "keywords": [
    "mds",
    "geographies"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-logger": "workspace:0.5.2",
    "@mds-core/mds-repository": "workspace:0.1.28",
    "@mds-core/mds-rpc-common": "workspace:0.2.10",
    "@mds-core/mds-schema-validators": "workspace:0.4.0",
    "@mds-core/mds-service-helpers": "workspace:0.4.7",
    "@mds-core/mds-test-data": "workspace:0.4.10",
    "@mds-core/mds-types": "workspace:0.8.0",
    "@mds-core/mds-utils": "workspace:0.2.14",
    "ajv": "8.6.3",
    "geojson-validation": "1.0.2",
    "joi": "17.4.0",
    "pg": "8.7.1",
    "typeorm": "0.2.40"
  }
}
