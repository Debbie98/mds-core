{
  "name": "@mds-core/mds-collector-api",
  "version": "0.1.28",
  "description": "Mobility Data Specification Collector API",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/collector pnpm watch server",
    "test": "PATH_PREFIX=/collector DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.54",
    "@mds-core/mds-api-server": "workspace:0.3.12",
    "@mds-core/mds-collector-backend": "workspace:0.2.4",
    "@mds-core/mds-logger": "workspace:0.4.4",
    "@mds-core/mds-service-helpers": "workspace:0.4.3",
    "@mds-core/mds-types": "workspace:0.7.1",
    "@mds-core/mds-utils": "workspace:0.2.10",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-schema-validators": "workspace:0.3.12"
  }
}
