{
  "name": "@mds-core/mds-policy-author",
  "version": "0.1.5",
  "description": "MDS Policy Author API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/policy-author pnpm watch server",
    "test": "PATH_PREFIX=/policy-author DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap  ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "0.1.33",
    "@mds-core/mds-api-server": "0.1.32",
    "@mds-core/mds-db": "0.2.1",
    "@mds-core/mds-logger": "0.2.3",
    "@mds-core/mds-providers": "0.1.32",
    "@mds-core/mds-schema-validators": "0.2.1",
    "@mds-core/mds-types": "0.3.1",
    "@mds-core/mds-utils": "0.1.32",
    "@types/express": "4.17.12",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "0.1.32"
  }
}
