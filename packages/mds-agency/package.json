{
  "name": "@mds-core/mds-agency",
  "version": "0.3.8",
  "description": "MDS Agency API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/agency pnpm watch server",
    "test": "PATH_PREFIX=/agency DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json --exit",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.4.6",
    "@mds-core/mds-api-helpers": "workspace:0.1.59",
    "@mds-core/mds-api-server": "workspace:0.3.15",
    "@mds-core/mds-db": "workspace:0.4.19",
    "@mds-core/mds-ingest-service": "workspace:0.6.9",
    "@mds-core/mds-logger": "workspace:0.5.1",
    "@mds-core/mds-providers": "workspace:0.2.0",
    "@mds-core/mds-schema-validators": "workspace:0.3.15",
    "@mds-core/mds-service-helpers": "workspace:0.4.5",
    "@mds-core/mds-stream": "workspace:0.2.1",
    "@mds-core/mds-types": "workspace:0.7.1",
    "@mds-core/mds-utils": "workspace:0.2.12",
    "@types/express": "4.17.13",
    "ajv": "8.6.1",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.7"
  }
