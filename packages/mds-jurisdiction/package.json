{
  "name": "@mds-core/mds-jurisdiction",
  "version": "0.1.14",
  "description": "MDS Jurisdictions API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/jurisdiction pnpm watch server",
    "test": "PATH_PREFIX=/jurisdiction DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap  ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.40",
    "@mds-core/mds-api-server": "workspace:0.3.2",
    "@mds-core/mds-jurisdiction-service": "workspace:0.1.11",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-service-helpers": "workspace:0.3.4",
    "@mds-core/mds-utils": "workspace:0.2.0",
    "@types/express": "4.17.12",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.3.0"
  }
}
