{
  "name": "@mds-core/mds-transaction-api",
  "version": "0.2.0",
  "description": "MDS Transaction API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "generate": "pnpm generate:schema && pnpm generate:flatspec",
    "generate:flatspec": "ts-node ./generate-flat-spec.ts && git add spec/flat-spec.json",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/transaction pnpm watch server",
    "test": "PATH_PREFIX=/transaction DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node -r ts-node/register -r dotenv/config",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.63",
    "@mds-core/mds-api-server": "workspace:0.3.18",
    "@mds-core/mds-logger": "workspace:0.5.2",
    "@mds-core/mds-schema-validators": "workspace:0.4.0",
    "@mds-core/mds-service-helpers": "workspace:0.4.7",
    "@mds-core/mds-transaction-service": "workspace:0.4.0",
    "@mds-core/mds-types": "workspace:0.8.0",
    "@mds-core/mds-utils": "workspace:0.2.14",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "joi": "17.4.0"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.10"
  }
}
