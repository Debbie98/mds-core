{
  "name": "@mds-core/mds-compliance-api",
  "version": "0.3.0",
  "description": "MDS Compliance API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "generate": "pnpm generate:flatspec",
    "generate:flatspec": "ts-node ./generate-flat-spec.ts && git add spec/flat-spec.json",
    "start": "PATH_PREFIX=/compliance-api pnpm watch server",
    "test": "PATH_PREFIX=/compliance-api DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.40",
    "@mds-core/mds-api-server": "workspace:0.3.2",
    "@mds-core/mds-compliance-service": "workspace:0.4.0",
    "@mds-core/mds-db": "workspace:0.4.0",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-policy-service": "workspace:0.5.0",
    "@mds-core/mds-providers": "workspace:0.1.35",
    "@mds-core/mds-schema-validators": "workspace:0.3.2",
    "@mds-core/mds-service-helpers": "workspace:0.3.4",
    "@mds-core/mds-test-data": "workspace:0.3.0",
    "@mds-core/mds-types": "workspace:0.5.0",
    "@mds-core/mds-utils": "workspace:0.2.0",
    "@types/express": "4.17.12",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "joi": "17.4.0"
  }
}
