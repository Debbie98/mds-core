{
  "name": "@mds-core/mds-compliance-api",
  "version": "0.4.11",
  "description": "MDS Compliance API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "generate": "pnpm generate:schema && pnpm generate:flatspec",
    "generate:flatspec": "ts-node ./generate-flat-spec.ts && git add spec/flat-spec.json",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/compliance-api pnpm watch server",
    "test": "PATH_PREFIX=/compliance-api DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.0",
    "@mds-core/mds-api-server": "workspace:0.4.0",
    "@mds-core/mds-compliance-service": "workspace:0.10.6",
    "@mds-core/mds-db": "workspace:0.4.32",
    "@mds-core/mds-logger": "workspace:0.5.5",
    "@mds-core/mds-policy-service": "workspace:0.11.4",
    "@mds-core/mds-providers": "workspace:0.2.5",
    "@mds-core/mds-schema-validators": "workspace:0.4.3",
    "@mds-core/mds-service-helpers": "workspace:0.6.0",
    "@mds-core/mds-test-data": "workspace:0.4.13",
    "@mds-core/mds-types": "workspace:0.8.2",
    "@mds-core/mds-utils": "workspace:0.2.17",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "joi": "17.4.0"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
