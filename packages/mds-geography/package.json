    "@mds-core/mds-logger": "workspace:0.5.1",
    "@mds-core/mds-providers": "workspace:0.2.0",
    "@mds-core/mds-schema-validators": "workspace:0.3.15",
    "@mds-core/mds-types": "workspace:0.7.1",
    "@mds-core/mds-utils": "workspace:0.2.12",
    "@types/express": "4.17.13",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.7"
  }
