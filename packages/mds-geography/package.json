{
  "name": "@mds-core/mds-geography",
  "version": "0.1.6",
  "description": "MDS Geography API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/geography pnpm watch server",
    "test": "PATH_PREFIX=/geography DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap  ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.47",
    "@mds-core/mds-api-server": "workspace:0.3.7",
    "@mds-core/mds-db": "workspace:0.4.7",
    "@mds-core/mds-logger": "workspace:0.4.1",
    "@mds-core/mds-providers": "workspace:0.1.40",
    "@mds-core/mds-schema-validators": "workspace:0.3.7",
    "@mds-core/mds-types": "workspace:0.5.5",
    "@mds-core/mds-utils": "workspace:0.2.5",
    "@types/express": "4.17.13",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.3.5"
  }
}
