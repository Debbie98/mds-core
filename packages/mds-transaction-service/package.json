{
  "name": "@mds-core/mds-transaction-service",
  "version": "0.7.4",
  "description": "MDS Transaction Service",
  "keywords": [
    "mds",
    "transactions"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "jest --runInBand",
    "build": "pnpm _build",
    "start": "pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "node --experimental-repl-await -r ts-node/register -r ../../dotenv-flow.js",
    "typeorm": "pnpm ts-node ./node_modules/typeorm/cli.js  -- -d ormconfig",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-ingest-service": "workspace:0.13.0",
    "@mds-core/mds-logger": "workspace:0.5.9",
    "@mds-core/mds-repository": "workspace:0.2.8",
    "@mds-core/mds-rpc-common": "workspace:0.3.25",
    "@mds-core/mds-schema-validators": "workspace:0.5.16",
    "@mds-core/mds-service-helpers": "workspace:0.6.12",
    "@mds-core/mds-stream": "workspace:0.4.4",
    "@mds-core/mds-types": "workspace:0.9.3",
    "@mds-core/mds-utils": "workspace:0.5.4",
    "envalid": "7.3.1",
    "pg": "8.7.3",
    "typeorm": "0.3.6",
    "typeorm-cursor-pagination": "0.6.1"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
