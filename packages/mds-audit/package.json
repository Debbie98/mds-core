{
  "name": "@mds-core/mds-audit",
  "version": "0.2.1",
  "description": "Endpoint for auditing compliance with MDS",
  "keywords": [
    "mds",
    "audit"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/audit pnpm watch server",
    "test": "PATH_PREFIX=/audit DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.2.9",
    "@mds-core/mds-api-helpers": "workspace:0.1.43",
    "@mds-core/mds-api-server": "workspace:0.3.5",
    "@mds-core/mds-attachment-service": "workspace:0.2.1",
    "@mds-core/mds-db": "workspace:0.4.3",
    "@mds-core/mds-ingest-service": "0.4.5",
    "@mds-core/mds-logger": "workspace:0.3.0",
    "@mds-core/mds-providers": "workspace:0.1.38",
    "@mds-core/mds-schema-validators": "workspace:0.3.5",
    "@mds-core/mds-service-helpers": "workspace:0.3.7",
    "@mds-core/mds-types": "workspace:0.5.3",
    "@mds-core/mds-utils": "workspace:0.2.3",
    "@types/express": "4.17.13",
    "@types/multer": "1.4.7",
    "@types/sharp": "0.29.2",
    "aws-sdk": "2.999.0",
    "express": "4.17.1",
    "multer": "1.4.3",
    "sharp": "0.29.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.3.3"
  }
}
