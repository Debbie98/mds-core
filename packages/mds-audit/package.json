{
  "name": "@mds-core/mds-audit",
  "version": "0.1.52",
  "description": "Endpoint for auditing compliance with MDS",
  "keywords": [
    "mds",
    "audit"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/audit pnpm watch server",
    "test": "PATH_PREFIX=/audit DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.2.6",
    "@mds-core/mds-api-helpers": "workspace:0.1.40",
    "@mds-core/mds-api-server": "workspace:0.3.2",
    "@mds-core/mds-attachment-service": "workspace:0.1.11",
    "@mds-core/mds-db": "workspace:0.4.0",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-providers": "workspace:0.1.35",
    "@mds-core/mds-schema-validators": "workspace:0.3.2",
    "@mds-core/mds-service-helpers": "workspace:0.3.4",
    "@mds-core/mds-types": "workspace:0.5.0",
    "@mds-core/mds-utils": "workspace:0.2.0",
    "@types/express": "4.17.12",
    "@types/multer": "1.4.6",
    "@types/sharp": "0.28.3",
    "aws-sdk": "2.939.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "sharp": "0.28.3"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.3.0"
  }
}
