{
  "name": "@mds-core/mds-ingest-service",
  "version": "0.4.3",
  "description": "Mobility Data Specification Ingest Service",
  "keywords": [
    "mds",
    "ingest"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.2.7",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-providers": "workspace:0.1.36",
    "@mds-core/mds-repository": "workspace:0.1.12",
    "@mds-core/mds-rpc-common": "workspace:0.1.12",
    "@mds-core/mds-schema-validators": "workspace:0.3.3",
    "@mds-core/mds-service-helpers": "workspace:0.3.5",
    "@mds-core/mds-stream": "workspace:0.1.38",
    "@mds-core/mds-types": "workspace:0.5.1",
    "@mds-core/mds-utils": "workspace:0.2.1",
    "joi": "17.4.0",
    "typeorm": "0.2.34",
    "typeorm-cursor-pagination": "0.6.1"
  }
}
