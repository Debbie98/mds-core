{
  "name": "@mds-core/mds-stream-processor",
  "version": "0.2.6",
  "description": "MDS Stream Processor",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "process": "pnpm watch",
    "start": "pnpm watch",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand --passWithNoTests",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-logger": "0.2.4",
    "@mds-core/mds-service-helpers": "0.3.3",
    "@mds-core/mds-stream": "0.1.36",
    "@mds-core/mds-types": "0.4.0",
    "@mds-core/mds-utils": "0.1.36",
    "@types/event-stream": "3.3.34",
    "event-stream": "4.0.1"
  }
}
