{
  "name": "@mds-core/mds-geography-author",
  "version": "0.1.0",
  "description": "MDS Geography Author API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/geography-author pnpm watch server",
    "test": "PATH_PREFIX=/geography-author DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap  ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.1.41",
    "@mds-core/mds-api-server": "workspace:0.3.3",
    "@mds-core/mds-db": "workspace:0.4.1",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-providers": "workspace:0.1.36",
    "@mds-core/mds-schema-validators": "workspace:0.3.3",
    "@mds-core/mds-types": "workspace:0.5.1",
    "@mds-core/mds-utils": "workspace:0.2.1",
    "@types/express": "4.17.12",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.3.1"
  }
}
