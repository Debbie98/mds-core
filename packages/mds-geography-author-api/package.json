{
  "name": "@mds-core/mds-geography-author-api",
  "version": "0.4.16",
  "description": "MDS Geography Author API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/geography-author pnpm watch server",
    "test": "PATH_PREFIX=/geography-author NODE_ENV=test DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap  ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.16",
    "@mds-core/mds-api-server": "workspace:0.4.8",
    "@mds-core/mds-geography-service": "workspace:0.10.13",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-schema-validators": "workspace:0.5.7",
    "@mds-core/mds-service-helpers": "workspace:0.6.6",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.4.2",
    "@types/express": "4.17.13",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.19"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
